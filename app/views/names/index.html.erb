<p id="notice"><%= notice %></p>

<h1>Names</h1>

<table>
  <thead>
    <tr>
      <th>Nconst</th>
      <th>Primaryname</th>
      <th>Birthyear</th>
      <th>Deathyear</th>
      <th>Primaryprofession</th>
      <th>Knownfortitles</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @names.each do |name| %>
      <tr>
        <td><%= name.nconst %></td>
        <td><%= name.primaryName %></td>
        <td><%= name.birthYear %></td>
        <td><%= name.deathYear %></td>
        <td><%= name.primaryProfession %></td>
        <td><%= name.knownForTitles %></td>
        <td><%= link_to 'Show', name %></td>
        <td><%= link_to 'Edit', edit_name_path(name) %></td>
        <td><%= link_to 'Destroy', name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Name', new_name_path %>
