<p id="notice"><%= notice %></p>

<h1>Principals</h1>

<table>
  <thead>
    <tr>
      <th>Tconst</th>
      <th>Nconst</th>
      <th>Category</th>
      <th>Characters</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @principals.each do |principal| %>
      <tr>
        <td><%= principal.tconst %></td>
        <td><%= principal.nconst %></td>
        <td><%= principal.category %></td>
        <td><%= principal.characters %></td>
        <td><%= link_to 'Show', principal %></td>
        <td><%= link_to 'Edit', edit_principal_path(principal) %></td>
        <td><%= link_to 'Destroy', principal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Principal', new_principal_path %>
